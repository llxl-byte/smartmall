32 |         name: 'OrderManagement',
33 |         component: () => import('../views/OrderManagement.vue'),
34 |         meta: { title: '订单管理' }
35 |       }
36 |       // 更多子路由...
37 |     ]
38 |   },
39 |   // 可以添加其他顶层路由，例如登录页面 (不需要布局)
40 |   // {
41 |   //   path: '/login',
42 |   //   name: 'Login',
43 |   //   component: () => import('../views/Login.vue')
44 |   // }
