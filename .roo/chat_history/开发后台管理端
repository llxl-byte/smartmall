 1 | import { createRouter, createWebHistory } from 'vue-router'
 2 | import BasicLayout from '../layout/BasicLayout.vue' // 导入基础布局组件
 3 | 
 4 | // 不再需要内联模板定义
 5 | 
 6 | const routes = [
 7 |   {
 8 |     path: '/', // 根路径指向布局
 9 |     component: BasicLayout, // 使用 BasicLayout 作为顶层组件
10 |     children: [ // 定义子路由
11 |       {
12 |         path: '', // 当访问 '/' 时，默认显示 Dashboard
13 |         name: 'Dashboard',
14 |         // 使用路由懒加载导入实际组件
15 |         component: () => import('../views/Dashboard.vue'),
16 |         meta: { title: '仪表盘' }
17 |       },
18 |       {
19 |         path: '/users', // 路径为 /users
