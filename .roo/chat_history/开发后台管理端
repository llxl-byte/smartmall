 4 | // import Dashboard from '../views/Dashboard.vue'
 5 | 
 6 | const routes = [
 7 |   {
 8 |     path: '/', // 根路径
 9 |     name: 'Dashboard', // 路由名称 (可选，但推荐)
10 |     // component: Dashboard // 指向 Dashboard 组件
11 |     // 暂时先使用一个简单的内联模板作为占位符
12 |     component: { template: '<div>仪表盘页面占位符</div>' }
13 |   },
14 |   // 其他路由规则将在这里添加...
15 |   // 例如：
16 |   // {
17 |   //   path: '/users',
18 |   //   name: 'UserManagement',
19 |   //   component: () => import('../views/UserManagement.vue') // 路由懒加载
20 |   // },
21 | ]
