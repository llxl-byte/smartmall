  <path>smart_mall_li_cr_springboot2/src/main/resources/application.yml</path>
  <content>
 1 | #告诉mybatis xml放在那
 2 | mybatis:
 3 |   #classpath代表的是resources文件夹
 4 |   mapper-locations: classpath:com/example/smart_mall_li_cr_springboot2/mapper/*.xml
 5 | server:
 6 |   port: 8083
 7 |   # # 允许从任何主机访问
 8 |   # address: 0.0.0.0
 9 | spring:
10 |   main:
11 |     allow-circular-references: true
12 |   datasource:
13 |     #      有的mysql如5.0 下面必须改成com.mysql.jdbc.Driver
14 |     driver-class-name: com.mysql.cj.jdbc.Driver
15 |     #改为3306端口
