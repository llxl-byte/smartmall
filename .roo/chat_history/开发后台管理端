 1 | // 位置：com.example.smart_mall_li_cr_springboot2.common包下
 2 | package com.example.smart_mall_li_cr_springboot2.common;
 3 | 
 4 | public class Result {
 5 |     private boolean success;   // 操作是否成功
 6 |     private String message;    // 提示信息
 7 |     private Object data;       // 数据（可存放用户对象等）
 8 | 
 9 |     // 无参构造器（可选）
10 |     public Result() {}
11 | 
12 |     // 成功时的构造器
13 |     public Result(boolean success, String message, Object data) {
14 |         this.success = success;
15 |         this.message = message;
16 |         this.data = data;
17 |     }
18 | 
19 |     // 失败时的构造器
20 |     public Result(boolean success, String message) {
21 |         this(success, message, null);
22 |     }
23 | 
24 |     // Getter 和 Setter（可使用 Lombok 的 @Data 注解简化）
25 |     public boolean isSuccess() { return success; }
26 |     public void setSuccess(boolean success) { this.success = success; }
27 |     public String getMessage() { return message; }
28 |     public void setMessage(String message) { this.message = message; }
29 |     public Object getData() { return data; }
30 |     public void setData(Object data) { this.data = data; }
31 | }
32 | 
  </content>
</file>
