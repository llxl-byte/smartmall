 76 |           // --- 可选：存储 token 或用户信息 ---
 77 |           // 例如，如果后端在 data 中返回了 token:
 78 |           // if (response.data.data && response.data.data.token) {
 79 |           //   localStorage.setItem('admin-token', response.data.data.token)
 80 |           // }
 81 |           // ------------------------------------
 82 |           router.push('/') // 跳转到仪表盘页面
 83 |         } else {
 84 |           // 如果 code 不是 '200' 或数据结构不符合预期，显示后端返回的错误消息
 85 |           ElMessage.error(response.data.msg || '用户名或密码错误')
 86 |         }
 87 |       } catch (error) {
 88 |         // 处理请求本身发生的错误 (例如网络问题、后端服务未启动等)
 89 |         console.error('Login request failed:', error)
 90 |         ElMessage.error('登录请求失败，请检查网络或联系管理员')
